{
  "name": "spending-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev2": "nodemon app.js --ignore client",
    "build": "npm run build --prefix client",
    "server": "nodemon",
    "client": "npm start --prefix client",
    "prod": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Shane Zhu & Eileen Zhang",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.16.4",
    "express-http-proxy": "^1.6.3",
    "express-session": "^1.17.3",
    "http-errors": "^1.8.1",
    "mongodb": "^4.11.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
